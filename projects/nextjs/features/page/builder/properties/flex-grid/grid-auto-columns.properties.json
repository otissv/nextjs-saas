{
  "gridAutoColumns": {
    "cols-auto": {
      "class": "auto-cols-auto",
      "property": "grid-auto-columns: auto;"
    },
    "min": {
      "class": "auto-cols-min",
      "property": "grid-auto-columns: min-content;"
    },
    "max": {
      "class": "auto-cols-max",
      "property": "grid-auto-columns: max-content;"
    },
    "fr": {
      "class": "auto-cols-fr",
      "property": "grid-auto-columns: minmax(0, 1fr);"
    }
  }
}
