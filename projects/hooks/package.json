{
  "name": "hooks",
  "private": true,
  "version": "1.0.0",
  "author": "Otis Virginie",
  "license": "MIT",
  "scripts": {
    "start": "npx http-server ./docs",
    "build": "npm run clean && mkdir build && ts-node node_modules/scripts/libs/build-package",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:coverage": "npm run build && jest --coverage",
    "test:watch": "jest --watch",
    "docs": "npm run build && typedoc libs/index.ts",
    "clean": "rimraf ./build",
    "prepare": "npm run build",
    "publish": "cd build && npm publish",
    "purge": "rimraf node_modules"
  },
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@types/react": "18.0.24",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-custom": "workspace:*",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-disable": "^2.0.3",
    "eslint-plugin-functional": "^4.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.0.3",
    "jsdoc": "^3.6.11",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "scripts": "workspace:*",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "database": "workspace:*",
    "utils": "workspace:*",
    "next": "^13.4.8",
    "react": "18.2.0",
    "typedoc": "^0.24.4"
  }
}
